{
	"name": "haneko",
	"version": "3.0.0",
	"description": "nHentai Discord Bot",
	"main": "dist/cluster.js",
	"scripts": {
		"dev": "npm run build && cross-env NODE_ENV=dev node dist/cluster.js",
		"start": "npm run build && node dist/cluster.js",
		"build": "rimraf dist && tsc",
		"build:watch": "tsc --watch",
		"lint": "eslint src/**/* --ext .ts",
		"lint:fix": "eslint src/**/* --ext .ts --fix",
		"fmt": "prettier --write src/**/*.{ts,json,yaml,yml}",
		"test": "jest",
		"diagnose": "node --version && npm --version && npx tsc --version && echo 'TypeScript files found:' && find src -name '*.ts' | head -10"
	},
	"type": "module",
	"license": "MIT",
	"keywords": [
		"nHentai"
	],
	"dependencies": {
		"@shineiichijo/nhentai-ts": "yeci226/nhentai-ts",
		"better-sqlite3": "^12.2.0",
		"discord-hybrid-sharding": "^2.2.6",
		"discord.js": "^14.21.0",
		"dotenv": "^17.2.1",
		"moment-timezone": "^0.6.0",
		"ms": "^2.1.3",
		"npm-check-updates": "^18.0.2",
		"os": "^0.1.2",
		"queue": "^7.0.0",
		"quick.db": "^9.1.7"
	},
	"devDependencies": {
		"@babel/core": "^7.28.0",
		"@babel/preset-env": "^7.28.0",
		"@types/better-sqlite3": "^7.6.13",
		"@types/node": "^24.2.0",
		"@typescript-eslint/eslint-plugin": "^8.39.0",
		"@typescript-eslint/parser": "^8.39.0",
		"cross-env": "^10.0.0",
		"eslint": "^9.32.0",
		"jest": "^30.0.5",
		"prettier": "^3.6.2",
		"rimraf": "^6.0.1",
		"typescript": "^5.9.2"
	}
}
